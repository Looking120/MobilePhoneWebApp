@model MobilePhoneWebApp.BusinessLogic.Dtos.OnlineStoreDto;

<!DOCTYPE html>
<html>
<head>
    <title>Create</title>
</head>
<body>

    <div class="container mt-5">
        <div class="text-center">
            <h1 class="display-5 mb-3">
                <strong>Create OnlineStore</strong>
            </h1>
        </div>
    </div>

    <div class="main row justify-content-center">

        <form asp-action="Create" method="post" class="row justify-content-center mb-4" autocomplete="off">

            <div class="main row justify-content-center">

                <form asp-action="Create" method="post" class="row justify-content-center mb-4" autocomplete="off">

                    <div class="col-7 mb-3">
                        <label for="Website">Website</label>
                        <input class="form-control" type="text" asp-for="WebSite" placeholder="Enter Website" />
                    </div>
                    <div class="col-7 mb-3">
                        <label for="Email">Website Mail</label>
                        <input class="form-control" type="text" asp-for="Email" placeholder="Enter Website mail" />
                    </div>
                    <div class="col-7 mb-3">
                        <label for="PhoneNuber"> Phone Number</label>
                        <input class="form-control" type="text" asp-for="PhoneNumber" placeholder="Enter Phone Number" />
                    </div>
                    <div class="col-7 mb-3">
                        <label for="Address">Address</label>
                        <input class="form-control" type="text" asp-for="Address" placeholder="Enter Address Website" />
                    </div>
                    <div class="col-7">
                        <button type="submit" class="btn btn-primary">Delete</button>
                    </div>
                </form>

            </div>

        </form>

    </div>
</body>
</html>
